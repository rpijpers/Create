	opacity: 1.00;
}

.harbour-pagination {
	text-align: left;
	margin: 0;
	padding: 3px 0;
	float: right;
}

.harbour-pagination li {
	display: inline;
	margin-right: 5px;
	font-size: 0.9em;
	
}

.harbour-pagination li span {
	text-decoration: none;
	
	background-color: #F6F6F6;
	padding: 3px;
	
	border: 1px solid #ddd;
	border-radius: 2px;
	-webkit-border-radius: 2px;
	-moz-border-radius: 2px;
}

.harbour-pagination li a {
	color: #0B55C4;
	text-decoration: none;
	
	background-color: #F6F6F6;
	padding: 3px;
	
	border: 1px solid #ddd;
	border-radius: 2px;
	-webkit-border-radius: 2px;
	-moz-border-radius: 2px;	
}

.harbour-pagination li a:hover, .harbour-pagination li a.selected {
	background-color: grey;
	color: #FFF;
}

.harbour-pagination li a.active {
	background-color: grey;
	color: #FFF;
	font-weight: bold;
}

