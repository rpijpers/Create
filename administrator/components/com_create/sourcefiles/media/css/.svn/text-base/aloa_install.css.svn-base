.log {
	padding-left:195px;
	font-weight: normal;
}
#install {
	min-height: 50px;
	padding-top: 15px;
	padding-left: 200px;
	font-size: 12px;
	font-weight: normal;
	font-family: Verdana, Helvetica, Arial, sans-serif;

}
#install h2.working {
	padding-left: 20px;
	background: url(../images/spinner.gif) no-repeat;
	font-size: 12px;
	line-height: 25px;
	font-weight: normal;
	font-family: Verdana, Helvetica, Arial, sans-serif;

}
#install h2.finished {
	padding-left: 20px;
	font-size: 12px;
	line-height: 25px;
	font-weight: bold;
	font-family: Verdana, Helvetica, Arial, sans-serif;
	color: green;
	background: none;

}
.log h2::after {
	content: attr(title);
	font-size: 10px;
	padding-left: 20px;
	font-weight: normal;
	font-family: Verdana, Helvetica, Arial, sans-serif;
}
.log h2{
	content: attr(title);
	font-size: 10px;
	padding-left: 20px;
	font-weight: normal;
	font-family: Verdana, Helvetica, Arial, sans-serif;
	color: #666666;
}

/*start aloa grid*/

body table.adminlist tbody tr.row0:hover td,body table.adminlist tbody tr.row1:hover td  {
	background-color: #DBFEFF;
}
body table.adminlist {
	border-spacing: 0px 0px;
	border: 1px solid #E1E1E1;	
}
body table.adminlist,
body table.adminlist > thead:first-child,
body table.adminlist > thead:first-child > tr:first-child,
body table.adminlist > thead:first-child > tr:first-child > th:last-child {
	border-top-right-radius: 5px;
	-webkit-border-top-right-radius: 5px;
	-moz-border-radius-topright: 5px;
}
body table.adminlist,
body table.adminlist > *:last-child,
body table.adminlist > *:last-child > tr:last-child,
body table.adminlist > *:last-child > tr:last-child > td:last-child,
body table.adminlist > *:last-child > tr:last-child > th:last-child,
body table.adminlist > tfoot,
body table.adminlist > tfoot > tr:last-child,
body table.adminlist > tfoot > tr:last-child > td:last-child {
	border-bottom-right-radius: 5px;
	-webkit-border-bottom-right-radius: 5px;
	-moz-border-radius-bottomright: 5px;
}
body table.adminlist > tfoot + tbody:last-child,
body table.adminlist > tfoot + tbody:last-child > tr:last-child,
body table.adminlist > tfoot + tbody:last-child > tr:last-child > td:last-child {
	border-bottom-right-radius: 0px;
	-webkit-border-bottom-right-radius: 0px;
	-moz-border-radius-bottomright: 0px;
}
body table.adminlist,
body table.adminlist > *:last-child,
body table.adminlist > *:last-child > tr:last-child,
body table.adminlist > *:last-child > tr:last-child > td:first-child,
body table.adminlist > *:last-child > tr:last-child > th:first-child,
body table.adminlist > tfoot,
body table.adminlist > tfoot > tr:last-child,
body table.adminlist > tfoot > tr:last-child > td:first-child {
	border-bottom-left-radius: 5px;
	-webkit-border-bottom-left-radius: 5px;
	-moz-border-radius-bottomleft: 5px;
}
body table.adminlist > tfoot + tbody:last-child,
body table.adminlist > tfoot + tbody:last-child > tr:last-child,
body table.adminlist > tfoot + tbody:last-child > tr:last-child > td:first-child {
	border-bottom-left-radius: 0px;
	-webkit-border-bottom-left-radius: 0px;
	-moz-border-radius-bottomleft: 0px;
}
body table.adminlist,
body table.adminlist > thead:first-child,
body table.adminlist > thead:first-child > tr:first-child,
body table.adminlist > thead:first-child > tr:first-child > th:first-child {
	border-top-left-radius: 5px;
	-webkit-border-top-left-radius: 5px;
	-moz-border-radius-topleft: 5px;
}
body table.adminlist > thead:only-child > tr:last-child > th {
	border-bottom-width: 0;
}
.grid-count {
	text-align: right !important;
}
body table.adminlist > * > tr > * {
	padding-left: 5px;
	padding-right: 5px;
}
body table.adminlist > * > tr > *:first-child {
	padding-left: 10px;
}
body table.adminlist > * > tr > *:last-child {
	padding-right: 10px;
}
.adminlist + .adminlist, .adminlist + form {
	margin-top: 5px !important;
}
body table.adminlist tbody tr td {
	border-left: 0px hidden transparent !important;
	border-top: 0px hidden transparent !important;
	border-right: 0px hidden transparent !important;
	border-bottom: 1px solid #ECECEC !important;
}
body table.adminlist tbody tr.selected, 
body table.adminlist tbody tr.selected td, 
body table.adminlist tbody tr.selected:hover > td,
body table.adminlist:hover tbody tr.selected:hover > td {
	-webkit-transition: 0s all;
	background-color: #2B91E3 !important;
	color: #CDE7FD !important;
}
body table.adminlist tbody.selected-single tr.selected, 
body table.adminlist tbody.selected-single tr.selected td, 
body table.adminlist tbody.selected-single tr.selected:hover > td,
body table.adminlist:hover tbody.selected-single tr.selected:hover > td {
	-webkit-box-shadow: inset #187dd3 0 1px 0, inset #004ba5 0 -1px 0;
	-moz-box-shadow: inset #187dd3 0 1px 0, inset #004ba5 0 -1px 0;
	box-shadow: inset #187dd3 0 1px 0, inset #004ba5 0 -1px 0;
	background-image: -webkit-gradient(linear, left top, left bottom, from(#2c8bdc), to(#0e67ca));
	background-image: -moz-linear-gradient(top, #2c8bdc, #0e67ca);
	text-shadow: rgba(0, 0, 0, 0.33) 0 1px 0;
}
body table.adminlist tbody tr.selected td, 
body table.adminlist tbody tr.selected:hover > td,
body table.adminlist:hover tbody tr.selected:hover > td {
	border-bottom-color: white;
}
body table.adminlist > tbody:last-child > tr:last-child td {
	border-bottom-width: 0 !important;
}
body.inactive table.adminlist tbody tr.selected, 
body.inactive table.adminlist tbody tr.selected td, 
body.inactive table.adminlist tbody tr.selected:hover > td,
body.inactive table.adminlist:hover tbody tr.selected:hover > td {
	-webkit-box-shadow: inset rgb(151, 151, 151) 0 1px 0;
	background-image: -webkit-gradient(linear, left top, left bottom, from(rgb(180, 180, 180)), to(rgb(138, 138, 138)));
	background-image: -moz-linear-gradient(top, rgb(180, 180, 180), rgb(138, 138, 138));
}
body table.adminlist tbody tr.selected a, 
body table.adminlist tbody tr.selected td a, 
body table.adminlist tbody tr.selected:hover > td a,
body table.adminlist:hover tbody tr.selected:hover > td a {
	color: white;
}
body table.adminlist thead tr,
body table.adminlist thead tr th,
body table.adminlist tfoot tr,
body table.adminlist tfoot tr td {
	background: white -webkit-gradient(linear, 0% 0%, 0% 100%, from(hsla(0, 0%, 96%, 0)), to(hsla(0, 0%, 96%, 0.796875)));
	background: white -moz-linear-gradient(top, hsla(0, 0%, 96%, 0), hsla(0, 0%, 96%, 0.796875));
}
body table.adminlist thead tr th,
body table.adminlist tfoot tr td {
	border-color: #E1E1E1;
}
body table.adminlist thead tr th:first-child {
	border-left-width: 0;
}

/*end aloa grid*/