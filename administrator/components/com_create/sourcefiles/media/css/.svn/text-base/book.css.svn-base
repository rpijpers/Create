/* CSS Eigenschaften */
#parent {
        width: 100%;
                /*Mozilla Präfix -moz-*/
                -moz-column-count:2;
                -moz-column-rule: solid 1px #f2f2f2;
                -moz-column-gap: 29px;
                /*Webkit Präfix -webkit-*/
                -webkit-column-count:2;
                -webkit-column-rule: solid 1px #f2f2f2;
                -webkit-column-gap: 29px;
        padding: 0 2px;
		margin-top: 15px;
        }

.media
{
	font-size: 12px;
	margin: 5px;
	margin-left:5px;
	margin-top: 0px;
	margin-right: 10px;
	text-align: right;
	float:left;	
	width: 156px;
	padding: 5px;	
	border: 2px solid #ddd;
	background-color: transparent;
	border-radius: 4px;
	-webkit-border-radius: 4px;
	-moz-border-radius: 4px;
}

.media img
{
	-ms-filter:"progid:DXImageTransform.Microsoft.Alpha(Opacity=75)";
	filter: alpha(opacity=75);
	opacity: .75;
}

.media img:hover
{
	-ms-filter:"progid:DXImageTransform.Microsoft.Alpha(Opacity=100)";
	filter: alpha(opacity=100);
	opacity: 1.00;
}

.media-nav
{
font-size: 12px;
	margin: 5px;
	margin-left:5px;
	margin-top: 0px;
	text-align: right;
	padding: 5px;	
	border: 1px solid #ddd;
	background-color: transparent;
	border-radius: 4px;
	-webkit-border-radius: 4px;
	-moz-border-radius: 4px;	
	
}

body div #nav {
text-align: center;
}

#nav img
{
	-ms-filter:"progid:DXImageTransform.Microsoft.Alpha(Opacity=60)";
	filter: alpha(opacity=60);
	opacity: .60;
}

#nav img:hover
{
	-ms-filter:"progid:DXImageTransform.Microsoft.Alpha(Opacity=100)";
	filter: alpha(opacity=100);
	opacity: 1.00;
}



.details
{
	margin-top: 5px;
	padding: 3px; 
	padding-left: 0px;
	float:right;
	font-size: 0.85em;
	font-style: italic;
	word-spacing:0.050em;
	letter-spacing:0.050em;
	color: #999;
	
font-family: "Adobe Caslon Pro", "Hoefler Text", Georgia, Garamond, Times, serif;


}

.harbour-pagination {
	text-align: left;
	margin: 0;
	padding: 0;
	float: right;
}

.harbour-pagination li {
	display: inline;
	margin-right: 5px;
	font-size: 0.9em;
	
}

.harbour-pagination li span {
	text-decoration: none;
	
	/*background-color: #F6F6F6;*/
	background-color: #eee;
	color: #bbb;
	padding: 3px;
	
	border: 1px solid #ddd;
	border-radius: 4px;
	-webkit-border-radius: 4px;
	-moz-border-radius: 4px;
}

.harbour-pagination li a {
	color: #0B55C4;
	text-decoration: none;
	
	background-color: #F6F6F6;
	padding: 3px;
	
	border: 1px solid #ddd;
	border-radius: 4px;
	-webkit-border-radius: 4px;
	-moz-border-radius: 4px;	
}

.harbour-pagination li a:hover, .harbour-pagination li a.selected {
	background-color: #bbb;
	color: #eee;
}

.harbour-pagination li a.active {
	background-color: grey;
	color: #FFF;
	font-weight: bold;
}


